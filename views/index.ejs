<!DOCTYPE html>
<html>
  <head>
      <% include ../views/partials/head %>   
  </head>
  <body>
    <header>
        <% include ../views/partials/header %>
    </header>
    
    <p>Welcome to Product</p>
        
    <% products.forEach(function(product) { %>  
    <div class="row">
            <% product.forEach(function(prod) { %>  
            <div class="col-sm-6 col-md-4">
                  <div class="thumbnail">
                    <img src="<%= prod.imagePath %>" alt="...">
                      <div class="caption">
                        <h3>Thumbnail label</h3>
                        <p><%= prod.description %></p>
                        <div class="clearfix">
                          <div class="price pull-left">$<%= prod.price %></div>
                        <a href="#" class="btn btn-primary pull-right" role="button">Add to Cart</a>
                        </div>                        
                      </div>
                  </div>      
            </div>
             <% }); %>      
   </div>
    <% }); %>


     <footer>
        <% include ../views/partials/footer %>
    </footer>
 
  </body>
</html>
